# vi: ai et ts=4 sw=4 sts=4 expandtab fs=shell

PATH=$(echo $PATH | tr ':' '\n' | grep -v -e '^$' | grep -v -E 'gnupg' | tr '\n' ':')
PATH=$(echo $PATH | tr ':' '\n' | grep -v -e '^$' | grep -v -E 'password-store' | tr '\n' ':')

if [[ -d "${HOME}/scoop/apps/gnupg/current" ]]; then
    export PATH
    PATH=${HOME}/scoop/apps/gnupg/current/bin:${PATH}
fi

if [[ -d "${HOME}/bin/password-store" ]]; then
    export PATH
    PATH=${PATH}:${HOME}/bin/password-store/bin
fi
