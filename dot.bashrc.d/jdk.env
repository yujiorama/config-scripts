PATH=$(echo "$PATH" | tr ':' '\n' | grep -v -e '^$' | grep -v -E 'jdk|adopt|java' | tr '\n' ':')

export JAVA_OPTS
JAVA_OPTS="-Dfile.encoding=UTF-8"

export JAVA_TOOL_OPTIONS
JAVA_TOOL_OPTIONS="-Dfile.encoding=UTF-8"

export JDK8=adopt8-hotspot
export JDK9=adopt9-openj9
export JDK10=adopt10-hotspot
export JDK11=adopt11-openj9
export JDK12=adopt12-hotspot
export JDK13=adopt13-openj9
export JDK14=openjdk14
